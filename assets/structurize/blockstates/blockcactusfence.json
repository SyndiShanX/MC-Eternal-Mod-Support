{
    "multipart": [
        {   "apply": { "model": "structurize:block/blockcactus_fence_post" }},
        {   "when": { "north": true },
            "apply": { "model": "structurize:block/blockcactus_fence_side_ne", "uvlock": false }
        },
        {   "when": { "east": true },
            "apply": { "model": "structurize:block/blockcactus_fence_side_ne", "y": 90, "uvlock": false }
        },
        {   "when": { "south": true },
            "apply": { "model": "structurize:block/blockcactus_fence_side_sw", "y": 180, "uvlock": false }
        },
        {   "when": { "west": true },
            "apply": { "model": "structurize:block/blockcactus_fence_side_sw", "y": 270, "uvlock": false }
        }
    ]
}